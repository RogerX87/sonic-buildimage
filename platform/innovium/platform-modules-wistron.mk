# Wistron Platform modules

WISTRON_3306_32_PLATFORM_MODULE_VERSION = 1.0
WISTRON_6512_32R_PLATFORM_MODULE_VERSION = 1.0

export WISTRON_3306_32_PLATFORM_MODULE_VERSION WISTRON_6512_32R_PLATFORM_MODULE_VERSION

WISTRON_3306_32_PLATFORM_MODULE = sonic-platform-wistron-3306-32_$(WISTRON_3306_32_PLATFORM_MODULE_VERSION)_$(CONFIGURED_ARCH).deb
$(WISTRON_3306_32_PLATFORM_MODULE)_SRC_PATH = $(PLATFORM_PATH)/sonic-platform-modules-wistron
$(WISTRON_3306_32_PLATFORM_MODULE)_DEPENDS += $(LINUX_HEADERS) $(LINUX_HEADERS_COMMON)
$(WISTRON_3306_32_PLATFORM_MODULE)_PLATFORM = x86_64-wistron_3306_32-r0

WISTRON_6512_32R_PLATFORM_MODULE = sonic-platform-wistron-6512-32r_$(WISTRON_6512_32R_PLATFORM_MODULE_VERSION)_$(CONFIGURED_ARCH).deb
$(WISTRON_6512_32R_PLATFORM_MODULE)_SRC_PATH = $(PLATFORM_PATH)/sonic-platform-modules-wistron
$(WISTRON_6512_32R_PLATFORM_MODULE)_DEPENDS += $(LINUX_HEADERS) $(LINUX_HEADERS_COMMON)
$(WISTRON_6512_32R_PLATFORM_MODULE)_PLATFORM = x86_64-wistron_6512_32r-r0

WISTRON_PLATFORM_MODULE = $(WISTRON_3306_32_PLATFORM_MODULE) $(WISTRON_6512_32R_PLATFORM_MODULE)

SONIC_DPKG_DEBS += $(WISTRON_PLATFORM_MODULE)
