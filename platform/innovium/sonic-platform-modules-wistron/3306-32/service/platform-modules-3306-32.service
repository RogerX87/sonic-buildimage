[Unit]
Description=Wistron 3306-32 platform initialization service
After=sysinit.target
Before=pmon.service
DefaultDependencies=no

[Service]
Type=oneshot
ExecStart=/etc/init.d/sonic-platform-wistron-3306-32 start
ExecStop=/etc/init.d/sonic-platform-wistron-3306-32 stop
RemainAfterExit=yes

[Install]
WantedBy=multi-user.target
